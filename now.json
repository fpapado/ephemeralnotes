{
  "version": 2,
  "name": "ephemeral-v2",
  "alias": [
    "ephemeralnotes.app",
    "ephemeral-v2"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {"distDir": "dist"}
    }
  ],
  "routes": [
    {
      "src": "^/(main-.*)",
      "dest": "/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/(leaflet-.*)",
      "dest": "/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/(storage-.*)",
      "dest": "/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/(system-.*)",
      "dest": "/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/(vendors~.*)",
      "dest": "/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/workbox-(.*)",
      "dest": "/workbox-$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/sw.js",
      "dest": "/sw.js",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "src": "^/assets/fonts/(.*)",
      "dest": "/assets/fonts/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/assets/images/(.*)",
      "dest": "/assets/images/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/(precache-manifest.*.js)",
      "dest": "/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {"src": "^/robots.txt", "dest": "/robots.txt"},
    {"src": ".*", "dest": "/"}
  ]
}
