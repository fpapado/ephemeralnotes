{
  "version": 2,
  "name": "ephemeral-v2",
  "alias": ["ephemeralnotes.app", "ephemeral-v2"],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {"distDir": "dist"}
    }
  ],
  "routes": [
    {
      "src": "^/sw.js",
      "dest": "/sw.js",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "src": "^/(.*.js)",
      "dest": "/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
    {
      "src": "^/(.*.css)",
      "dest": "/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/workbox-(.*)",
      "dest": "/workbox-$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/assets/fonts/(.*)",
      "dest": "/assets/fonts/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/assets/images/(.*)",
      "dest": "/assets/images/$1",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/assets/manifest.json",
      "dest": "/assets/manifest.json",
      "headers": {
        "cache-control": "must-revalidate, max-age=3600"
      }
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {"src": "^/robots.txt", "dest": "/robots.txt"},
    {"src": ".*", "dest": "/"}
  ]
}
