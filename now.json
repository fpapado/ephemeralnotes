{
  "version": 2,
  "name": "ephemeral-v2",
  "alias": "ephemeral-v2",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {"distDir": "dist"}
    }
  ],
  "routes": [
    {
      "src": "main-.*",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "leaflet-.*",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "vendors~.*",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/workbox-.*/.*.js",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/assets/fonts/phantom-sans-v.*",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/assets/images/marker.*",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "precache-manifest.*.js",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "sw.js",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {"src": ".*", "dest": "/"}
  ]
}
