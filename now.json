{
  "name": "ephemeral-v2",
  "alias": "ephemeral-v2",
  "type": "static",
  "scale": {
    "bru1": {
      "min": 0,
      "max": 1
    },
    "gru1": {
      "min": 0,
      "max": 1
    },
    "sfo1": {
      "min": 0,
      "max": 1
    }
  },
  "features": { "cloud": "v2" },
  "static": {
    "headers": [
      {
        "source": "main-*.@(css|js)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000,immutable"
          }
        ]
      },
      {
        "source": "workbox-*/*.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000,immutable"
          }
        ]
      },
      {
        "source": "phantom-sans-v*-*.@(woff|woff2)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000,immutable"
          }
        ]
      },
      {
        "source": "precache-manifest.*.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000,immutable"
          }
        ]
      },
      {
        "source": "sw.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ]
  }
}
